<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<!-- Ladda -->
<script src="js/plugins/ladda/spin.min.js"></script>
<script src="js/plugins/ladda/ladda.min.js"></script>
<script src="js/plugins/ladda/ladda.jquery.min.js"></script>

<script>
$(document).ready(function(){
    $('.product-images').slick({
        dots: true
    });

    let btnDeployHelloworld = $('#deploy_helloworld').ladda()
    let btnDeployVNC = $('#deploy_vnc').ladda()

    btnDeployHelloworld.click(function(){
        btnDeployHelloworld.ladda('start');
        axios.post('/products/deploy', {
            type: 'helloworld'
        })
        .then(function(response){
            console.log(response)
            btnDeployHelloworld.ladda('stop');
            if (response.data.result == 'success') {
                swal({
                    title: "Deployed successfully!",
                    text: "It will take some time to configure service. Please wait... Service URL is http://" + response.data.address.fqdn,
                    type: "success"
                });
            }
        })
        .catch(function(error){
            btnDeployHelloworld.ladda('stop');
            console.log(error)
        })
    })

    btnDeployVNC.click(function(){
        btnDeployVNC.ladda('start');
        axios.post('/products/deploy', {
            type: 'vnc'
        })
        .then(function(response){
            btnDeployVNC.ladda('stop');
            console.log(response)
            if (response.data.result == 'success') {
                swal({
                    title: "Deployed successfully!",
                    text: "It will take some time to configure service. Please wait... Service URL is http://" + response.data.address.fqdn,
                    type: "success"
                });
            }
        })
        .catch(function(error){
            btnDeployVNC.ladda('stop');
            console.log(error)
        })
    })
});
</script>
